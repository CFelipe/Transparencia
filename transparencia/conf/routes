# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

GET     /administrador              controllers.Administrador.index()
GET     /signIn                     controllers.Administrador.signIn()
POST    /doLogin                    controllers.Administrador.doLogin()
POST    /createParlamentar          controllers.Administrador.createParlamentar()
POST    /createProcesso          	controllers.Administrador.createProcesso()

#QUEM FOI A RAPARIGA QUE EXCLUIU A EXCLUSAO DO PARLAMENTAR?

GET		/addSessao					controllers.Administrador.sessoes()
POST	/addSessao					controllers.Administrador.addSessao()
POST	/addSessao/:id/delete		controllers.Administrador.excluirSessao(id: Long)

GET     /parlamentar/:id            controllers.Parlamentar.listarUm(id: Long)
GET     /processo/:id            	controllers.Processo.listarUm(id: Long)



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
